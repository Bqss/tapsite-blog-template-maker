{
  "title": "{{post.title}} - Creative Studio",
  "description": "{{post.excerpt}}",
  "keywords": [
    "creative blog",
    "design tutorials",
    "{{post.category}}",
    "{{post.tags}}"
  ],
  "page_type": "post",
  "sections": [
    {
      "title": "Navigation",
      "id": "navigation",
      "type": "template",
      "resetable": true,
      "data": {
        "blog_title": "blog_title",
        "blog_logo": "blog_logo",
        "navigation": "navigation"
      }
    },
    {
      "title": "Post Content",
      "id": "post_content",
      "type": "template",
      "resetable": true,
      "data": ["post", "author"]
    },
    {
      "title": "Related Posts",
      "id": "related_posts",
      "type": "template",
      "resetable": true
    },
    {
      "title": "Footer",
      "id": "footer",
      "type": "template",
      "resetable": true,
      "data": ["navigation", "blog_title", "blog_logo"]
    }
  ],
  "meta": {
    "robots": "index, follow",
    "canonical": "https://creativestudio.com/post/{{post.slug}}",
    "author": "{{post.author}}",
    "published_time": "{{post.published_at}}",
    "modified_time": "{{post.updated_at}}"
  },
  "structured_data": {
    "@context": "https://schema.org",
    "@type": "BlogPosting",
    "headline": "{{post.title}}",
    "description": "{{post.excerpt}}",
    "image": "{{post.featured_image}}",
    "author": {
      "@type": "Person",
      "name": "{{post.author}}"
    },
    "publisher": {
      "@type": "Organization",
      "name": "Creative Studio",
      "logo": {
        "@type": "ImageObject",
        "url": "{{site.logo}}"
      }
    },
    "datePublished": "{{post.published_at}}",
    "dateModified": "{{post.updated_at}}"
  }
}
