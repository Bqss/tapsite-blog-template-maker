{
  "seo": {
    "title": "{{post.title}} - LifestyleLux",
    "description": "{{post.excerpt}}",
    "keywords": "{{post.tags}}, luxury lifestyle, premium content",
    "canonical": "/post/{{post.slug}}",
    "og_title": "{{post.title}}",
    "og_description": "{{post.excerpt}}",
    "og_image": "{{post.featured_image}}",
    "og_type": "article",
    "twitter_card": "summary_large_image",
    "twitter_title": "{{post.title}}",
    "twitter_description": "{{post.excerpt}}",
    "twitter_image": "{{post.featured_image}}",
    "schema": {
      "@context": "https://schema.org",
      "@type": "Article",
      "headline": "{{post.title}}",
      "description": "{{post.excerpt}}",
      "image": "{{post.featured_image}}",
      "author": {
        "@type": "Person",
        "name": "{{post.author}}"
      },
      "publisher": {
        "@type": "Organization",
        "name": "LifestyleLux",
        "logo": {
          "@type": "ImageObject",
          "url": "{{site.url}}/logo.png"
        }
      },
      "datePublished": "{{post.published_at}}",
      "dateModified": "{{post.updated_at}}",
      "mainEntityOfPage": {
        "@type": "WebPage",
        "@id": "{{site.url}}/post/{{post.slug}}"
      }
    }
  },
  "sections": [
    {
      "title": "styles",
      "id": "styles",
      "type": "template",
      "resetable": true
    },
    {
      "title": "navigation",
      "id": "navigation",
      "type": "template",
      "resetable": true,
      "data": ["site", "navigation", "subscribe_button", "search_placeholder"]
    },
    {
      "title": "post_content",
      "id": "post_content",
      "type": "template",
      "resetable": true,
      "data": ["post", "author"]
    },
    {
      "title": "footer",
      "id": "footer",
      "type": "template",
      "resetable": true,
      "data": ["site", "navigation", "social_links", "footer_columns", "current_year"]
    }
  ]
} 